    "pretty-hrtime": "1.0.1",
    "require-dir": "0.3.0",
    "run-sequence": "1.1.5",
    "selenium-server": "2.51.0",
    "serve-static": "1.10.2",
    "stringify": "4.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.7.0",
    "yargs": "3.32.0"
  },
  "dependencies": {
    "axios": "0.9.1",
    "babel-polyfill": "6.5.0",
    "bootstrap-sass": "3.3.6",
    "fastclick": "1.0.6",
    "jquery": "2.2.0",
    "lodash": "4.3.0",
    "markdown": "0.5.0",
    "validator.js": "1.2.3",
    "vue": "1.0.16"
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-swap",
      "browserify-shim",
      [
        "stringify",
        {
          "extensions": [
            ".hbs",
            ".tpl",
            ".mustache",
            ".html"
          ],
          "minify": true
        }
      ]
    ]
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "browserify-swap": {
    "@packages": [
      "underscore"
    ],
    "dist": {
      "underscore.js$": "lodash"
    }
  }
}
