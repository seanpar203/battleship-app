{
  "name": "label-starter-app",
  "version": "0.5.2",
  "description": "HTML5 App Boilerplate",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "https://git.dev:7999/l4b3l/starter"
  },
  "keywords": [
    "label",
    "boilerplate",
    "gulp"
  ],
  "private": true,
  "author": "Jason Daly <jason@labelinteractive.com>",
  "devDependencies": {
    "babel": "^5.4.7",
    "babelify": "^6.1.2",
    "browser-sync": "^2.7.6",
    "browserify": "^10.2.3",
    "browserify-shim": "^3.8.7",
    "browserify-swap": "^0.2.2",
    "del": "^1.2.0",
    "dotenv": "^1.1.0",
    "gulp": "~3.9.0",
    "gulp-autoprefixer": "~2.3.1",
    "gulp-browserify": "~0.5.1",
    "gulp-changed": "^1.2.1",
    "gulp-filesize": "0.0.6",
    "gulp-filter": "~2.0.2",
    "gulp-fingerprint": "^0.3.2",
    "gulp-flatten": "~0.0.4",
    "gulp-if": "~1.2.5",
    "gulp-imagemin": "~2.2.1",
    "gulp-jshint": "~1.11.0",
    "gulp-minify-css": "1.1.5",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-preprocess": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-rev-all": "^0.8.21",
    "gulp-rev-napkin": "^0.1.0",
    "gulp-sass": "^2.0.1",
    "gulp-size": "~1.2.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-strip-debug": "^1.0.2",
    "gulp-template": "^3.0.0",
    "gulp-uglify": "~1.2.0",
    "gulp-util": "^3.0.5",
    "jshint-stylish": "~2.0.0",
    "merge-stream": "^0.1.7",
    "pretty-hrtime": "^1.0.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.1",
    "yargs": "~3.10.0"
  },
  "dependencies": {
    "backbone": "^1.2.0",
    "bootstrap-sass": "^3.3.4",
    "fastclick": "^1.0.6",
    "jquery": "^2.1.4",
    "lodash": "^3.9.3",
    "markdown": "^0.5.0",
    "node-bourbon": "~4.2.3",
    "node-neat": "~1.7.2",
    "validator.js": "^1.1.2"
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "loose": [
            "es6.modules",
            "es6.classes"
          ]
        }
      ],
      "browserify-swap",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "browserify-swap": {
    "@packages": [
      "underscore"
    ],
    "dist": {
      "underscore.js$": "lodash"
    }
  }
}
