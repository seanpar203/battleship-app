    "type": "git",
    "url": "https://git.dev:7999/l4b3l/starter"
  },
  "keywords": [
    "label",
    "boilerplate",
    "gulp"
  ],
  "private": true,
  "author": "Jason Daly <jason@labelinteractive.com>",
  "devDependencies": {
    "babel": "6.5.1",
    "babel-eslint": "5.0.0-beta6",
