    "connect-modrewrite": "0.8.5",
    "del": "2.2.0",
    "dotenv": "2.0.0",
    "finalhandler": "0.4.1",
    "gulp": "3.9.0",
    "gulp-autoprefixer": "3.1.0",
    "gulp-browserify": "0.5.1",
    "gulp-changed": "1.3.0",
    "gulp-cssnano": "2.1.0",
    "gulp-eslint": "1.1.1",
    "gulp-filesize": "0.0.6",
    "gulp-filter": "3.0.1",
    "gulp-fingerprint": "0.3.2",
    "gulp-flatten": "0.2.0",
    "gulp-htmlmin": "1.3.0",
    "gulp-if": "2.0.0",
    "gulp-ignore": "2.0.1",
    "gulp-imagemin": "2.4.0",
