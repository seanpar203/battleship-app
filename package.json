    "gulp-rev-all": "0.8.22",
    "gulp-rev-napkin": "0.1.0",
    "gulp-sass": "2.2.0",
    "gulp-size": "2.0.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-strip-debug": "1.1.0",
    "gulp-template": "3.1.0",
    "gulp-uglify": "1.5.2",
    "gulp-util": "3.0.7",
    "http": "0.0.0",
    "imagemin-pngquant": "4.2.0",
    "karma": "0.13.19",
    "karma-browserify": "5.0.1",
    "karma-chrome-launcher": "0.2.2",
