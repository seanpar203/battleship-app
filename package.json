  "dependencies": {
    "axios": "^0.9.0",
    "babel-polyfill": "^6.2.0",
    "bootstrap-sass": "^3.3.5",
    "fastclick": "^1.0.6",
    "jquery": "^2.1.4",
    "lodash": "^4.0.0",
    "markdown": "^0.5.0",
    "validator.js": "^1.2.2",
    "vue": "^1.0.10"
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-swap",
      "browserify-shim",
      [
        "stringify",
        {
          "extensions": [
            ".hbs",
            ".tpl",
            ".mustache",
            ".html"
          ],
          "minify": true
        }
      ]
    ]
  },
  "browserify-shim": {
    "bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "browserify-swap": {
    "@packages": [
      "underscore"
    ],
    "dist": {
      "underscore.js$": "lodash"
    }
  }
}
